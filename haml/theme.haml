%head
  %title {Title}

  %meta{:charset => "utf-8"}
  {block:Description}
  %meta{:name => "description", :content => "{MetaDescription}"}
  {/block:Description}
  %meta{:name => "viewport", :content => "width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0"}

  %link{:rel => "canonical", :href => "{block:IndexPage}{BlogURL}{/block:IndexPage}{block:PermalinkPage}{Permalink}{/block:PermalinkPage}"}/
  %link{:rel => "alternate", :type => "application/rss+xml", :href => "{RSS}"}

  / Favicon
  %link{:rel => "shortcut icon", :href => "{Favicon}", :type => "image/x-icon"}/
  %link{:rel => "icon", :href => "{Favicon}", :type => "image/x-icon"}/
  %link{:rel => "apple-touch-icon", :href => "{PortraitURL-128}"}/

  / CSS
  %link{:rel => "stylesheet", :href => "https://www.dropbox.com/s/plsf52y57fbu6h8/main.css?dl=1", :type => "text/css", :media => "screen"}/

  / JS
  %script{:src => "//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js", :type => "text/javascript"}
  -# %script{:src => "your script link", :type => "text/javascript"}

%body
  %header.header-main
    .wrapper
      {block:ShowHeaderImage}
      .blog-cover{:style => "background-image: url({HeaderImage});"}
      {/block:ShowHeaderImage}
      .blog-mast.row
        .blog-title.single-col
          %h1.logo
            %a{:href => "{BlogURL}"} {Title}
        {block:Description}
        .blog-desc.single-col
          %p {Description}
        {/block:Description}
        .blog-nav.single-col
          %nav.main-nav
            %ul
              {block:HasPages}
              {block:Pages}
              %li
                %a{:href => "{URL}"} {Label}
              {/block:Pages}
              {/block:HasPages}
    
  #page-content
    .wrapper
      .posts
        {block:Posts}
        %article{:class => "row single-post {PostType} {TagsAsClasses}"}

          -# Title Column --------------------------
          .post-title.single-col
            .post-title-block
            {block:Date}
            %h3.post-date
              %a{:href => "{BlogURL}/day/{Year}/{MonthNumberWithZero}/{DayOfMonthWithZero}"}
              {DayOfMonth} {Month} {Year}
            {/block:Date}

            -# Text Post
            {block:Text}
            {block:Title}
            %h2
              %a.permalink{:href => "{URL}"} {Title}
            {/block:Title}
            {/block:Text}

            -# Photo Post
            {block:Photo}
            {block:Caption}
            .caption-post-body
              {Caption}
            {/block:Caption}
            {/block:Photo}
            
            -# Panorama Post
            {block:Panorama}
            {block:Caption}
            .caption-post-body
              {Caption}
            {/block:Caption}
            {/block:Panorama}

            -# Photoset Post
            {block:Photoset}
            {block:Caption}
            .caption-post-body
              {Caption}
            {/block:Caption}
            {/block:Photoset}

            -# Video Post
            {block:Video}
            {block:Caption}
            .caption-post-body
              {Caption}
            {/block:Caption}
            {/block:Video}

            .post-meta
              {block:HasTags}
              .post-tags
                %h5.tags
                  {block:Tags}
                  %a{:href => "{TagURL}"} {Tag}
                  {/block:Tags}
              {/block:HasTags}
              {block:PostNotes}
              .post-notes
                %h5.note-count {NoteCountWithLabel}
              {/block:PostNotes}
              {block:Date}
              .post-interact
                {ReblogButton color="grey"}
                {LikeButton color="grey"}
              {/block:Date}



          -# Body Column --------------------------
          .post-body.double-col

            -# Text Post
            {block:Text}
            {block:Body}
            .text-post-body
              {Body}
            {/block:Body}
            {/block:Text}
          
          
            -# Photo Post
            {block:Photo}
            .photo-wrap
              %picture
                %source{:media => "(max-width: 500px)", :srcset => "{PhotoURL-500}"}
                %source{:media => "(min-width: 501px)", :srcset => "{PhotoURL-HighRes}"}
                %img{:src => "{PhotoURL-HighRes}", :alt => "{PhotoAlt}"}
              {block:Exif}
              .exif-trigger
              .exif-contain
                %h5 {block:Camera}{Camera}{/block:Camera}{block:FocalLength} / {FocalLength}{/block:FocalLength}{block:Aperture} / {Aperture}{/block:Aperture}{block:Exposure} / {Exposure}{/block:Exposure}{block:ISO} / {ISO}{/block:ISO}
              {/block:Exif}
              {block:LinkURL}
              .clickthrough
                {LinkOpenTag}{LinkURL}{LinkCloseTag}
              {/block:LinkURL}
            {/block:Photo}

            -# Panorama Post
            {block:Panorama}
            .photo-wrap
              %picture
                %source{:media => "(max-width: 500px)", :srcset => "{PhotoURL-500}"}
                %source{:media => "(min-width: 501px, max-width:1280px)", :srcset => "{PhotoURL-HighRes}"}
                %source{:media => "(min-width:1281px)", :srcset => "{PhotoURL-Panorama}"}
                %img{:src => "{PhotoURL-Panorama}", :alt => "{PhotoAlt}"}
              {block:Exif}
              .exif-trigger
              .exif-contain
                %h5 {block:Camera}{Camera}{/block:Camera}{block:FocalLength} / {FocalLength}{/block:FocalLength}{block:Aperture} / {Aperture}{/block:Aperture}{block:Exposure} / {Exposure}{/block:Exposure}{block:ISO} / {ISO}{/block:ISO}
              {/block:Exif}
              {block:LinkURL}
              .clickthrough
                {LinkOpenTag}{LinkURL}{LinkCloseTag}
              {/block:LinkURL}
            {/block:Panorama}
            
            -# Photoset Post
            {block:Photoset}
            .photoset-imgs-wrap
              .photoset-imgs
                {block:Photos}
                .photo-wrap
                  %picture
                    %source{:media => "(max-width: 500px)", :srcset => "{PhotoURL-500}"}
                    %source{:media => "(min-width: 501px)", :srcset => "{PhotoURL-HighRes}"}
                    %img{:src => "{PhotoURL-HighRes}", :alt => "{PhotoAlt}"}
                  {block:Exif}
                  .exif-trigger
                  .exif-contain
                    %h5 {block:Camera}{Camera}{/block:Camera}{block:FocalLength} / {FocalLength}{/block:FocalLength}{block:Aperture} / {Aperture}{/block:Aperture}{block:Exposure} / {Exposure}{/block:Exposure}{block:ISO} / {ISO}{/block:ISO}
                  {/block:Exif}
                  {block:Caption}
                  .photo-single-caption
                    {Caption}
                  {/block:Caption}
                {/block:Photos}
            {block:Caption}
            .caption-post-body
              {Caption}
            {/block:Caption}
            {/block:Photoset}
            
            -# Quote Post
            {block:Quote}
            %h1.quote-body {Quote}
            {block:Source}
            %h3.quote-source {Source}
            {/block:Source}
            {/block:Quote}
            
            -# Link Post
            {block:Link}
            %a.link-single{:href => "{URL}", :title => "{Name}", :target => "new"}
              {block:Thumbnail}
              {Thumbnail}
              {/block:Thumbnail}
              %h2 {Name}
              %h5 {URL}
              {block:Description}
              %p {Description}
              {/block:Description}
            {/block:Link}

            -# Chat Post
            {block:Chat}
            {block:Title} 
            %h2
              %a.permalink{:href => "{URL}"} {Title}
            {/block:Title}
            %ul.chat-content-body
              {block:Lines}
              %li{:class => "{Alt}"}
                {block:Label}
                .person
                  %h4 {Label
                {/block:Label}
                .line 
                  %p {Line}
              {/block:Lines}
            {/block:Chat}

            -# Audio Post
            {block:Audio}
            .audio-inner
              {block:AudioEmbed}
              .audio-embed
                {AudioEmbed}
              {/block:AudioEmbed}
              {block:AudioPlayer}
              .audio-player
                {AudioPlayer}
              {/block:AudioPlayer}
              {block:PlayCount}
              %h5.play-count {PlayCountWithLabel}
              {/block:PlayCount}
              {block:TrackName}
              .audio-info
                {block:AlbumArt}
                %img.album-art{:src => "{AlbumArtURL}", :alt => "{Artist} - {Album}"}/
                {/block:AlbumArt}
                %h4 {TrackName}
                {block:Artist}
                %h5 {Artist}
                {/block:Artist}
                {block:Album}
                %h5 {Album}
                {/block:Album}
              {/block:TrackName}
            {block:Caption}
            .caption-post-body
              {Caption}
            {/block:Caption}
            {/block:Audio}
            
            -# Video Post
            {block:Video}
            .video-inner
              .fitvids
                {VideoEmbed-700}
              %h5.play-count {PlayCountWithLabel}
            {/block:Video}

        {/block:Posts}

  %footer
    .wrapper
      {CopyrightYears}

</html>